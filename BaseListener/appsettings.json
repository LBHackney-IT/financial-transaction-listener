{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "XRay": {
    "DisableXRayTracing": "false",
    "UseRuntimeErrors": "true",
    "CollectSqlQueries": "false"
  },
  "AWS": {
    "Region": "eu-west-2"
  },
  // Remove if not using DynamoDb
  "DynamoDb": {
    "LocalMode": false
  }
}
